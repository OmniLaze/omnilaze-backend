{
  "family": "omnilaze-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "442729101249.dkr.ecr.ap-southeast-1.amazonaws.com/omnilaze-backend:4ea7977892adc88ded44f9174dcc01c93f244bd5",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "ALIYUN_REGION_ID",
          "value": "cn-hangzhou"
        },
        {
          "name": "ALIYUN_DYPN_ENDPOINT",
          "value": "dypnsapi.aliyuncs.com"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/DATABASE_URL"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/JWT_SECRET"
        },
        {
          "name": "CORS_ORIGINS",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/CORS_ORIGINS"
        },
        {
          "name": "ALIPAY_APP_ID",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/ALIPAY_APP_ID"
        },
        {
          "name": "ALIPAY_PRIVATE_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:442729101249:secret:omnilaze/ALIPAY_PRIVATE_KEY-xxxxx"
        },
        {
          "name": "ALIPAY_PUBLIC_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:442729101249:secret:omnilaze/ALIPAY_PUBLIC_KEY-yyyyy"
        },
        {
          "name": "ALIPAY_GATEWAY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/ALIPAY_GATEWAY"
        },
        {
          "name": "ALIPAY_NOTIFY_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/ALIPAY_NOTIFY_URL"
        },
        {
          "name": "ALIYUN_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/ALIYUN_ACCESS_KEY_ID"
        },
        {
          "name": "ALIYUN_ACCESS_KEY_SECRET",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/ALIYUN_ACCESS_KEY_SECRET"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/omnilaze-backend",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "backend"
        }
      }
    }
  ]
}
