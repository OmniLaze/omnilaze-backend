{
  "family": "omnilaze-migrations",
  "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "migrations",
      "image": "442729101249.dkr.ecr.ap-southeast-1.amazonaws.com/omnilaze-backend:0863ee98f83770fea7a8bb6218b6dae7cbe6f968",
      "cpu": 0,
      "essential": true,
      "command": ["npm", "run", "prisma:deploy"],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://postgres:Omnilaze2024!@omnilaze-postgres.czoamaem021x.ap-southeast-1.rds.amazonaws.com:5432/omnilaze"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/omnilaze-migrations",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    }
  ]
}