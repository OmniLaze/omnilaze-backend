{
  "family": "omnilaze-app",
  "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "442729101249.dkr.ecr.ap-southeast-1.amazonaws.com/omnilaze-backend:f1fc1c0",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/NODE_ENV"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/PORT"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/DATABASE_URL"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/JWT_SECRET"
        },
        {
          "name": "CORS_ORIGINS",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/CORS_ORIGINS"
        },
        {
          "name": "ADMIN_USERNAME",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ADMIN_USERNAME"
        },
        {
          "name": "ADMIN_PASSWORD",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ADMIN_PASSWORD"
        },
        {
          "name": "SYSTEM_API_KEY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/SYSTEM_API_KEY"
        },
        {
          "name": "ALIPAY_APP_ID",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_APP_ID"
        },
        {
          "name": "ALIPAY_PRIVATE_KEY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_PRIVATE_KEY"
        },
        {
          "name": "ALIPAY_PUBLIC_KEY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_PUBLIC_KEY"
        },
        {
          "name": "ALIPAY_GATEWAY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_GATEWAY"
        },
        {
          "name": "ALIPAY_NOTIFY_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_NOTIFY_URL"
        },
        {
          "name": "ALIPAY_RETURN_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/ALIPAY_RETURN_URL"
        },
        {
          "name": "WECHAT_GATEWAY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/WECHAT_GATEWAY"
        },
        {
          "name": "WECHAT_NOTIFY_URL",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:442729101249:parameter/omnilaze/prod/WECHAT_NOTIFY_URL"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/omnilaze-service",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
