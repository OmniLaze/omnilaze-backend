{
  "family": "omnilaze-app",
  "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "PLACEHOLDER_IMAGE_URI",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "secrets": [
        {
          "name": "ALIYUN_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIYUN_ACCESS_KEY_ID"
        },
        {
          "name": "ALIYUN_ACCESS_KEY_SECRET",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIYUN_ACCESS_KEY_SECRET"
        },
        {
          "name": "ALIYUN_REGION_ID",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIYUN_REGION_ID"
        },
        {
          "name": "ALIYUN_SMS_SIGN_NAME",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIYUN_SMS_SIGN_NAME"
        },
        {
          "name": "ALIYUN_SMS_TEMPLATE_CODE",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIYUN_SMS_TEMPLATE_CODE"
        },
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/NODE_ENV"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/PORT"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/DATABASE_URL"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/JWT_SECRET"
        },
        {
          "name": "CORS_ORIGINS",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/CORS_ORIGINS"
        },
        {
          "name": "ADMIN_USERNAME",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ADMIN_USERNAME"
        },
        {
          "name": "ADMIN_PASSWORD",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ADMIN_PASSWORD"
        },
        {
          "name": "SYSTEM_API_KEY",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/SYSTEM_API_KEY"
        },
        {
          "name": "ALIPAY_APP_ID",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_APP_ID"
        },
        {
          "name": "ALIPAY_PRIVATE_KEY",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_PRIVATE_KEY"
        },
        {
          "name": "ALIPAY_PUBLIC_KEY",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_PUBLIC_KEY"
        },
        {
          "name": "ALIPAY_GATEWAY",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_GATEWAY"
        },
        {
          "name": "ALIPAY_NOTIFY_URL",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_NOTIFY_URL"
        },
        {
          "name": "ALIPAY_RETURN_URL",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/ALIPAY_RETURN_URL"
        },
        {
          "name": "WECHAT_GATEWAY",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/WECHAT_GATEWAY"
        },
        {
          "name": "WECHAT_NOTIFY_URL",
          "valueFrom": "arn:aws:ssm:__AWS_REGION__:ACCOUNT_ID:parameter/omnilaze/ENVIRONMENT/WECHAT_NOTIFY_URL"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/omnilaze-service",
          "awslogs-region": "__AWS_REGION__",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
