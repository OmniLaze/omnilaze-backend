{
  "family": "omnilaze-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::442729101249:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "442729101249.dkr.ecr.ap-southeast-1.amazonaws.com/omnilaze-backend:amd64-20250818-212500",
      "essential": true,
      "portMappings": [{ "containerPort": 3000 }],
      "environment": [
        {"name": "PORT", "value": "3000"},
        {"name": "NODE_ENV", "value": "production"},
        {"name": "DATABASE_URL", "value": "postgresql://postgres:Omnilaze2024!@omnilaze-postgres.czoamaem021x.ap-southeast-1.rds.amazonaws.com:5432/omnilaze"},
        {"name": "CORS_ORIGINS", "value": "[\"https://order.omnilaze.co\", \"http://localhost:8081\", \"http://localhost:3000\"]"},
        {"name": "JWT_SECRET", "value": "omnilaze-jwt-secret-2024"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/omnilaze-service",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
